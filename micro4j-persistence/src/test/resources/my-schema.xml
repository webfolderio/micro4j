<schema xmlns="https://micro4j.com/micro4j-persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="https://micro4j.com/micro4j-persistence https://raw.githubusercontent.com/micro4j/micro4j/master/micro4j-persistence/src/main/resources/micro4j-persistence.xsd"
	name="MY_SCHEMA2" sequence="MY_SEQ">

	<table name="MY_TABLE">
		<column name="ID" type="BIGINT" primaryKey="true" />
		<column name="ACTIVE" type="BOOLEAN" />
		<column name="CREATE_DATE" type="TIMESTAMP" />
		<column name="UPDATE_DATE" type="TIMESTAMP" />
		<column name="CREATED_BY" size="32" type="VARCHAR" />
		<column name="UPDATED_BY" size="32" type="VARCHAR" />
		<column name="NAME" size="255" type="VARCHAR" nullable="true" />
	</table>

</schema>
